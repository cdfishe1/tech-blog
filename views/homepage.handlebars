<!-- Loop over each object in the projects array -->
{{#each articles as |article| }}

<div class="card">
            <div class="card-body ms-3">
                
              <h5 class="card-title bg-primary text-light p-3">{{article.title}}</h5>
              <div class="card-header">
                Posted by {{user.user_name}} on 5/7/2021
              </div>
              <p class="card-text ps-4">{{article.content}}</p>
            </div>
          </div>
{{/each}}